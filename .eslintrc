{
  "env": {
    "browser": true,
    "amd": true,
    "jasmine": true,
      "jquery": true
  },
  "globals": {
    "requirejs": true
  },
  "rules": {
    "comma-dangle": [2, "never"],
    "no-cond-assign": 2,
    "no-constant-condition" : 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-inner-declarations": [2, "both"],
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-regex-spaces": 2,
    "no-sparse-arrays": 2,
    "no-unreachable": 2,
    "use-isnan": 2,
    "valid-typeof": 2,

    "block-scoped-var": 2,
    "curly": 2,
    "dot-location": [2, "property"],
    "eqeqeq": [2, "smart"],
    "guard-for-in":   2,
    "no-alert": 2,
//    "no-console": 2,
    "no-case-declarations": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extra-bind": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-implicit-coercion": 2,
    "no-implied-eval": 2,
    "no-invalid-this": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,

//    "no-magic-numbers": 2,

    "no-multi-spaces": 2,
    "no-new-func": 2,
    "no-new": 2,
    "no-redeclare": 2,
    "no-return-assign": 2,
    "no-self-compare": 2,
    "no-throw-literal": 2,
    "no-unused-expressions": 2,
    "no-useless-call": 2,
    "no-useless-concat": 2,
    "no-with": 2,
    "vars-on-top": 2,
    "yoda": 2,

    "strict": 2,

    "no-delete-var": 2,
    "no-shadow": 2,
    "no-undef-init": 2,
    "no-undef": 2,
    "no-undefined": 2,
    "no-unused-vars": 2,
    "no-use-before-define": [2, "nofunc"],

    "array-bracket-spacing": 2,
    "block-spacing": 2,
    "brace-style": 2,
    "camelcase": 2,
    "comma-spacing": 2,
    "comma-style": 2,
    "computed-property-spacing": 2,
    "consistent-this": [2, "that"],
    "eol-last": 2,
    "indent": [2, 4, {"SwitchCase": 1}],
    "key-spacing": 2,
    "lines-around-comment": 2,

//    "max-params": [2, 3],

//    "max-statements": [2, 1],

    "new-cap": 2,
    "new-parens": 2,
    "newline-after-var": 2,
    "no-array-constructor": 2,

//    "no-lonely-if": 2,

    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": 2,
    "no-negated-condition": 2,
    "no-new-object": 2,
    "no-plusplus": 2,
    "no-spaced-func": 2,
    "no-trailing-spaces": [2, { "skipBlankLines": true }],
    "no-unneeded-ternary": 2,
    "object-curly-spacing": 2,
    "one-var": 2,
    "operator-assignment": 2,
    "operator-linebreak": 2,
    "quotes": [2, "single", "avoid-escape"],

    "require-jsdoc": 2,

    "semi-spacing": 2,
    "semi": 2,
    "keyword-spacing": 2,
    "space-before-blocks": 2,
    "space-before-function-paren": 2,
    "space-in-parens": 2,
    "space-infix-ops": 2,
    "space-unary-ops": 2,
    "spaced-comment": 2,
    "wrap-regex": 2
  }
}